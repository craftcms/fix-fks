{
  "name": "craftcms/fix-fks",
  "description": "Restore missing foreign key constraints in the database.",
  "version": "1.0.2.1",
  "type": "craft-plugin",
  "keywords": [
    "db",
    "database",
    "cms",
    "craftcms",
    "yii2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pixel & Tonic",
      "homepage": "https://pixelandtonic.com/"
    }
  ],
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/craftcms/fix-fks/issues?state=open",
    "source": "https://github.com/craftcms/fix-fks",
    "docs": "https://github.com/craftcms/fix-fks/blob/main/README.md",
    "rss": "https://github.com/craftcms/fix-fks/commits/v2.atom"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/cms": "^3.0.23"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "craft\\fixfks\\": "src/"
    }
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "extra": {
    "name": "Fix FKs",
    "handle": "fix-fks",
    "documentationUrl": "https://github.com/craftcms/fix-fks/blob/main/README.md"
  },
  "config": {
    "platform": {
      "php": "7.2.5"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
